/*导航键样式*/
QPushButton#cur_mode:checked,QPushButton#vel_mode:checked,QPushButton#pos_mode:checked,QPushButton#profile_mode:checked,
QPushButton#homing_mode:checked{
        background-color: #111d2b;
        color:white;
}
QPushButton#cur_mode:pressed,QPushButton#vel_mode:pressed,QPushButton#pos_mode:pressed,QPushButton#profile_mode:pressed,
QPushButton#homing_mode:pressed{
        background-color: #111d2b
}

QPushButton#cur_mode,QPushButton#vel_mode,QPushButton#pos_mode,QPushButton#profile_mode,QPushButton#homing_mode{
        border : none;
        background-color: #1d2d3c;
        color:#878088;
        font:13px;
        opacity:50;
}

QWidget{
    outline:none;
}

